/*
7. Calcula el promedio de 3 notas para n estudiantes.
*/

#include <iostream>
using namespace std;
int main(int argc, char *argv[]) {
int a[10];
int b=0;
cout<<"INGRESE 3 NUMEROS "<<endl;
for(int i=0;i<3;i++){
cin>>a[i];
}
for(int j=0;j<3;j++){
b=b+a[j];
}
cout<<"promedio de las 3 notas "<<b/3<<endl;
return 0;
}
