/*
4. Escriba un c칩digo que solicite ingresar dos n칰meros 洧논 y 洧녽, tal que 洧논 < 洧녽. Muestre todos los
n칰meros primos que se encuentren entre el rango de valores, de no encontrarse, mostrar el
primo m치s cercano a 洧논 o 洧녽.
*/

#include <iostream>
using namespace std;
int divi(int a){
int cont;
int b=0;
for(int i=1;i<a;i++){
cont=a%i;
if(cont==0){
b++;
}
}
return b;
}
int main(int argc, char *argv[]) {
int a,b;
cout<<"INGRESE 2 NUMEROS "<<endl;
cin>>a>>b;
for(int i=a;i<b;i++){
if(divi(i)==1)
cout<<i<<" ";
}
return 0;
}
