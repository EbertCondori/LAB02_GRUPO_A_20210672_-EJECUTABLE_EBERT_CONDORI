/*
2. Calcule los primeros 50 n√∫meros primos y muestre el resultado en pantalla.
*/

#include <iostream>
using namespace std;

int divi(int a){
int cont;
int b=0;
for(int i=1;i<a;i++){
cont=a%i;
if(cont==0){
b++;
}
}
return b;
}
int main(int argc, char *argv[]) {
for(int i=0;i<50;i++){
if(divi(i)==1)
cout<<i<<" ";
}
return 0;
}
